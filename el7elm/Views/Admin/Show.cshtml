@model IEnumerable<el7elm.Models.admin>

@{
    ViewBag.Title = "Show";
}

<h2>Show Admins</h2>

<p>
    @Html.ActionLink("Add New Admin", "Add_Admin")
</p>

<table class="table">
    <tr>
        <th>
            Name 
        </th>
        <th>
            Email
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Admin_Edit", new { id=item.admin_id }) |
            @Html.ActionLink("Details", "Admin_Details", new { id=item.admin_id }) |
            
        </td>
    </tr>
}

</table>
