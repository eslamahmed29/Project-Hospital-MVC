@model IEnumerable<el7elm.Models.avail_appointments>

@{
    ViewBag.Title = "Show_Avail";
}

<h2>Show Available Appointments </h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Doctor ID 
        </th>
        <th>
            Start Date
        </th>
        <th>
            End Date 
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.doc_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.start_avail_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.end_avail_date)
        </td>
        
    </tr>
}

</table>
