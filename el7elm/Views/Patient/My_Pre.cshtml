@model IEnumerable<el7elm.Models.prescription>

@{
    ViewBag.Title = "My_Pre";
}

<h2>My Prescription</h2>


<table class="table">
    <tr>
        <th>
            Patient ID
        </th>
        <th>
            Doc ID
        </th>
        <th>
           Description
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.patient_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.doc_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        
    </tr>
}

</table>
